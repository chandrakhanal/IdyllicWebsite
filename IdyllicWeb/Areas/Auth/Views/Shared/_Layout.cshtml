<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1.0, user-scalable=no">
    <title>@ViewBag.Title</title>
    <link rel="icon" href="~/Content/img/favicon.ico" type="image/x-icon">
    <!-- Required Fremwork -->

    <link href="~/Content/bootstrap.min.css" rel="stylesheet" id="bootstrap-css" />
     <script src="~/Scripts/bootstrap.min.js"></script><link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.6.0.min.js"></script>
    
    <link href="~/Content/sitecss/authstyle.css" rel="stylesheet" />
    <script type="text/javascript" src="~/Areas/Admin/Component/scripts/ccryptft.js"></script>
    <script type="text/javascript" src="~/Areas/Admin/Component/scripts/ccryptimp.js"></script>
</head>
<body>
    <div class="container h-100">
        <div class="d-flex justify-content-center h-100">
            <div class="user_card">
                <div class="d-flex justify-content-center">
                    <div class="brand_logo_container">
                        <img src="~/Content/img/logo/techbrills-logo-trans.png" class="brand_logo" alt="Logo">
                    </div>
                </div>
                
                    @RenderBody()
                                   
                    <div class="text-center opacity-8 mt-3">Copyright © @DateTime.Now.Year Techbrills</div>
                </div>
            </div>
        </div>
    @RenderSection("scripts", required: false)
</body>
    </html>
