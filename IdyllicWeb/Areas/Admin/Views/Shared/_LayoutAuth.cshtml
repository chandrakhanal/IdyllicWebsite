<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1.0, user-scalable=no">
    <title>@ViewBag.Title</title>
    <link rel="icon" href="/images/NDC-Logo.png" type="image/x-icon">
    <!-- Required Fremwork -->
    <script src='~/Component/js/jquery/jquery.min.js' type="text/javascript"></script>
    <link type="text/javascript" href="~/Component/js/jquery/jquery.min.js">
    <link rel="stylesheet" type="text/css" href="~/Component/css/bootstrap.min.css">
    <script src='~/Component/js/bootstrap.min.js' type="text/javascript"></script>
    <link rel="stylesheet" type="text/css" href="~/Areas/Admin/Component/css/main.css">
    <script type="text/javascript" src="~/Areas/Admin/Component/scripts/ccryptft.js"></script>
    <script type="text/javascript" src="~/Areas/Admin/Component/scripts/ccryptimp.js"></script>
</head>
    <body>
        <div class="app-container app-theme-white body-tabs-shadow">
            <div class="app-container">
                <div class="h-100 bg-4 bg-animation">
                    <div class="d-flex h-100 justify-content-center align-items-center">
                        <div class="mx-auto app-login-box col-md-6">
                            <div class="app-logo-inverse mx-auto mb-3">
                            </div>
                            @RenderBody()
                            <div class="text-center opacity-8 mt-3">Copyright © @DateTime.Now.Year NDC > <a href="http://createch-software.com">Developed by Createch Software Pvt. Ltd.</a></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        @RenderSection("scripts",required:false)
    </body>
    </html>
